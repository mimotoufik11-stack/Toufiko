{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["src/layouts/*"],
      "@scripts/*": ["scripts/*"]
    }
  },
  "exclude": ["dist", "node_modules"]
}
